{
  "name": "kabouter",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "scripts": {
    "build": "npx tsc",
    "watch": "npx tsc --watch",
    "test": "npx ava --verbose",
    "test-browser": "npx aristotle watch -t test/browser/index.tsx -p 3010",
    "build-browser": "npx aristotle build -t test/browser/index.tsx -d test/browser/dist"
  },
  "repository": "https://github.com/atelier-saulx/kabouter",
  "lint-staged": {
    "{js,json}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "ava": {
    "files": [
      "test/**/*.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "peerDependencies": {
    "react": "18|19"
  },
  "prettier": "@saulx/prettier-config",
  "eslintConfig": {
    "extends": "@saulx/eslint-config"
  },
  "dependencies": {
    "@saulx/hash": "^2.0.0",
    "@saulx/utils": "^3.2.1"
  },
  "devDependencies": {
    "esbuild": "0.17.10",
    "react-dom": "^18.2.0",
    "@saulx/aristotle-server": "^1.1.0",
    "@saulx/eslint-config": "https://github.com/atelier-saulx/eslint-config",
    "@saulx/prettier-config": "https://github.com/atelier-saulx/prettier-config",
    "ava": "3.15.0",
    "eslint": "^7.18.0",
    "husky": "^3.0.8",
    "@saulx/aristotle": "^4.6.0",
    "react": "^18.2.0",
    "@types/react": "^18.0.28",
    "@types/node": "^18.0.0",
    "lint-staged": "^9.4.2",
    "prettier": "^2.2.1",
    "ts-node": "8.5.4",
    "typescript": "^4.7.4"
  }
}
